{
  "name": "kalatori",
  "description": "DOT payments module",
  "homepage": "https://github.com/Alzymologist/kalatori",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "Alzymologist",
      "email": "info@zymologia.fi"
    }
  ],
  "require": {
    "php": ">=5.5"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "^3.4"
  },
  "autoload": {
    "classmap": [
      "kalatori.php"
    ],
    "exclude-from-classmap": []
  },
  "config": {
    "preferred-install": "dist",
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "prepend-autoloader": false
  },
  "type": "prestashop-module"
}
